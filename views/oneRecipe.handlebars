    <title> Delicious Recipe </title>
    <link rel="stylesheet" href="../css/recipeStyle.css">
    <header>
      <section class="wrapper">
        <nav class="navBar">
          <div class="navBarLeft">
            <ul>
              <li>
                <a href=#home><img src="../img/logo.png" class="logo"></a>
              </li>
              <li><a href=#home> Spacer </a>
              </li>
              <li><a href="calTrack.html"> Calorie Calculator </a>
              </li>
              <li><a href="mBoard.html"> Message Board </a>
              </li>
              <li><a href="recipes.html"> Recipes </a>
              </li>
            </ul> 
          </div>
          <div class="navBarRight">
            <a href="dashboard.html"> Hi, {{username}} </a>
          </div> 
        </nav>
        <div class="landing">
          <img src="{{img}}" class="landImg">
        </div>
      </section>
    </header>
    <main>
      <div class="recipe-name"> 
        <h1> {{recipeName}} </h1>
      </div>
      <div class="ingredientList">
        <ul>
          {{#each ingredient}}
          <li class="ing">
            {{this.quantity}} {{this.metric}} {{this.ingredient_name}}
          </li>
          {{/each}}
        <ul>
      </div>
      <div class="recipe-instructions">
        <p> {{recipeInstructions}}  
        </p>
      </div>
    </main>
    <footer>
      <div class="center">
        <div id="flogo">
          <a href=#home><img src="../img/logo.png" alt="logo"></a>
        </div>
        <div id="links">
          <h2> CHECK OUT THESE GREAT FEATURES!
          </h2>
          <ul>
            <li><a href=#calTrack> Calorie Tracker </a>
            </li>
            <li><a href=#mBoard> Message Board </a>
            </li>
          </ul>
          <div class="credit">
            <h4> Recipe credit: {{recipeSource}} </h4>
            <h4> Logo credit: Sariah Bunnell with the help of Icons Producer and 
             Guilherme Furtado via the Noun Project 
            </h4>
          </div>
        </div>
      </div>
    </footer>
