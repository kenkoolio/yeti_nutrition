   <main>
      <div class="greeting"> 
        <h1> Welcome, {{username}}! </h1>
        <h2> Check out all your features here! </h2>
      </div>
      <div class="console">
        <div class="button">
          <a href="/recipes"> 
            <div class="dashImg">
              <span class="dashName"> See Recipes </span>
              <img src="/icon/recipeBook.png">
              <img src="/icon/recipeBook2.png" class="colorChange">
            </div>
          </a>  
        </div>         
        <div class="button">
          <a href="/posts"> 
            <div class="dashImg">
              <span class="dashName"> Message Board </span>
              <img src="/icon/message.png" alt="default">
              <img src="/icon/message2.png" class="colorChange">
            </div>
          </a>  
        </div>
        <div class="button">
          <a href="/calories"> 
            <div class="dashImg">
              <span class="dashName"> Track Calories </span>
              <img src="/icon/calTrack.png">
              <img src="/icon/calTrack2.png" class="colorChange">
            </div>
          </a>  
        </div>
      </div>
    </main>
